{
  "name": "@exa-labs/fasttext-wasm",
  "version": "1.1.0",
  "description": "Webassembly version of fastText (exa fork with noEos support)",
  "homepage": "https://github.com/exa-labs/fastText.wasm#readme",
  "bugs": {
    "url": "https://github.com/exa-labs/fastText.wasm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exa-labs/fastText.wasm.git"
  },
  "license": "MIT",
  "author": {
    "name": "Exa Labs",
    "url": "https://exa.ai"
  },
  "contributors": [
    {
      "name": "冰雪殇璃陌梦 (original author)",
      "email": "admin@dreamofice.cn",
      "url": "https://www.dreamofice.cn"
    }
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/core/fasttext.mjs": "./dist/core/fasttext.mjs",
    "./dist/core/fasttext.wasm": "./dist/core/fasttext.wasm",
    "./dist/model/lid.176.ftz": "./dist/model/lib.176.ftz"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run \"/^build:.*/\"",
    "build:core": "xmake build",
    "build:main": "tsup",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "eslint --ext .ts --fix ./src",
    "lint:prettier": "prettier --write .",
    "prepare": "husky install",
    "release": "dotenv release-it",
    "test": "vitest"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@release-it/conventional-changelog": "^7.0.0",
    "@tsconfig/node-lts": "^18.12.3",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "prettier": "^3.0.1",
    "prettier-plugin-packagejson": "^2.4.5",
    "release-it": "^16.1.3",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "pnpm": {},
  "engines": {
    "node": ">=14.15.0"
  }
}
